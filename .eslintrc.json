{
    "extends": "standard",
    "plugins": [
        "svelte3"
    ],
    "overrides": [
        {
            "files": ["**/*.svelte"],
            "processor": "svelte3/svelte3",
            "rules": {
                "import/first": "off",
                "import/no-duplicates": "off",
                "import/no-mutable-exports": "off",
                "import/no-unresolved": "off"
              }
        }
    ],
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
    }
}