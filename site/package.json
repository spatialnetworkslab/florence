{
  "name": "florence-site",
  "description": "Site for Florence",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev --ext '.svelte .sveltex'",
    "build": "sapper build --ext '.svelte .sveltex'",
    "export": "sapper export --ext '.svelte .sveltex' > /dev/null",
    "start": "node __sapper__/build",
    "watch:tailwind": "postcss src/tailwind.css -o static/css/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/tailwind.css -o static/css/index.css"
  },
  "dependencies": {
    "@polka/send": "^1.0.0-next.3",
