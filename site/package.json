    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@snlab/florence": "^0.1.20",
    "@snlab/florence-datacontainer": "^0.1.9",
    "@snlab/sveltex-unified": "^0.0.16",
    "d3-contour": "^1.3.2",
    "d3-fetch": "^1.1.2",
    "d3-format": "^1.4.4",
    "d3-interpolate": "^1.4.0",
    "d3-scale": "^3.2.1",
