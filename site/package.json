{
  "name": "florence-site",
  "description": "Site for Florence",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev --ext '.svelte .sveltex'",
    "build": "sapper build --ext '.svelte .sveltex'",
    "export": "sapper export --ext '.svelte .sveltex' > /dev/null",
    "start": "node __sapper__/build",
    "watch:tailwind": "postcss src/tailwind.css -o static/css/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/tailwind.css -o static/css/index.css"
  },
  "dependencies": {
    "@polka/send": "^1.0.0-next.3",
    "compression": "^1.7.1",
    "polka": "^1.0.0-next.4",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@snlab/florence": "^0.1.19",
    "@snlab/florence-datacontainer": "^0.1.6",
    "@snlab/sveltex-unified": "0.0.14",
    "d3-contour": "^1.3.2",
    "d3-fetch": "^1.1.2",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "purgecss-from-html": "^2.1.0",
    "purgecss-from-svelte": "^2.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.10",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.5.0",
    "sync-rpc": "^1.3.6",
    "tailwindcss": "^1.2.0"
  }
}
